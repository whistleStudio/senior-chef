{
    "recipes": [
        {
            "name": "清炒西兰花配胡萝卜丝",
            "ingredients": [
                {
                    "name": "西兰花",
                    "amount": "300克"
                },
                {
                    "name": "胡萝卜",
                    "amount": "150克"
                },
                {
                    "name": "橄榄油",
                    "amount": "15毫升"
                },
                {
                    "name": "蒜末",
                    "amount": "5克"
                },
                {
                    "name": "盐",
                    "amount": "2克"
                }
            ],
            "steps": [
                "西兰花掰成小朵，用清水浸泡10分钟后洗净。",
                "胡萝卜切成细丝备用。",
                "锅中加水烧开，放入西兰花焯水1分钟，捞出沥干。",
                "热锅倒入橄榄油，加入蒜末爆香。",
                "放入胡萝卜丝翻炒至变软，再加入西兰花翻炒均匀。",
                "调入盐，翻炒约1分钟即可出锅。"
            ],
            "nutrients": {
                "维生素B9（叶酸）": {
                    "amount": "280微克",
                    "nrv": "70%"
                }
            }
        },
        {
            "name": "蒸南瓜配藜麦",
            "ingredients": [
                {
                    "name": "南瓜",
                    "amount": "400克"
                },
                {
                    "name": "藜麦",
                    "amount": "100克"
                },
                {
                    "name": "清水",
                    "amount": "200毫升"
                },
                {
                    "name": "橄榄油",
                    "amount": "10毫升"
                },
                {
                    "name": "黑胡椒粉",
                    "amount": "1克"
                }
            ],
            "steps": [
                "南瓜去皮切块，放入蒸锅蒸约15分钟至熟透。",
                "藜麦提前浸泡30分钟，冲洗干净后加水煮沸，转小火焖煮15分钟至熟。",
                "将蒸好的南瓜与煮好的藜麦混合，淋上橄榄油，撒上黑胡椒粉拌匀即可。"
            ],
            "nutrients": {
                "维生素B9（叶酸）": {
                    "amount": "160微克",
                    "nrv": "40%"
                }
            }
        }
    ]
}